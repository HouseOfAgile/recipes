###> houseofagile/hoa-companion ###
TAG=0.1.0

APP_NAME=%generate(name)%
APP_MAIN_DOMAIN=`%generate(name)%-hoa-companion.flensburger.houseofagile.com`,`dev.%generate(name)%-hoa-companion.flensburger.houseofagile.com`
APP_WEB_SERVER_PORT=33080
APP_INNET_SERVER_PORT=33081

COMPOSE_PROJECT_NAME=%generate(name)%_dev

# Symfony Environment Variables
APP_ENV=dev
APP_SECRET=ChangeMe

# Database Configuration (PostgreSQL for frankenphp stack)
POSTGRES_HOST=database
POSTGRES_USER=app
POSTGRES_PASSWORD=!ChangeMe!
POSTGRES_DB=app
POSTGRES_VERSION=16
DATABASE_URL=postgresql://app:!ChangeMe!@database:5432/app?serverVersion=16

# Redis Configuration
REDIS_URL=redis://redis:6379

# Mailer Configuration
MAILER_DSN=smtp://mailhog:1025

# Messenger Configuration
MESSENGER_TRANSPORT_DSN=doctrine://default?auto_setup=0

# CORS Configuration
CORS_ALLOW_ORIGIN='^https?://(localhost|127\\.0\\.0\\.1)(:[0-9]+)?$'

# Sentry Configuration
SENTRY_DSN=
